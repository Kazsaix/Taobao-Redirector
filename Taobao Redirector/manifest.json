{
  "manifest_version": 2,
  "name": "Taobao Link Converter",
  "description": "This extension changes mobile and world taobao links to normal links",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "*://*.taobao.com/*",
    "*://taobao.com/*",
    "webRequestBlocking"
  ],
  "background":{"scripts":["background.js"]},
  "browser_action": {
    "default_icon": "icon128.png"
  }
}
